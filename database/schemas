-----configureEnricher-----
CREATE TABLE config
(
  appid serial NOT NULL DEFAULT 0,
  owner text,
  logo text,
  menu json,
  url text,
  name text,
  CONSTRAINT appid PRIMARY KEY (appid)
)

-----apis Tracker-----
CREATE TABLE apistracker
(
  request json,
  response json,
  timeconsumed text,
  id serial NOT NULL,
  CONSTRAINT id PRIMARY KEY (id)
)